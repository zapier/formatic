{
  "name": "formatic",
  "version": "1.1.11",
  "description": "Automatic, pluggable form generation",
  "main": "./build/lib/index.js",
  "scripts": {
    "build": "babel lib --out-dir build/lib && rollup -c",
    "watch": "babel lib --out-dir build/lib && rollup -c -w",
    "docs-build": "gulp docs-build",
    "docs-publish": "gulp docs-publish",
    "format": "zapier-scripts format '__tests__/**/*.js' 'demo/**/*.js' 'lib/**/*.js' 'scripts/**/*.js' 'tasks/**/*.js' '*.js'",
    "lint": "zapier-scripts lint '__tests__/**/*.js' 'demo/**/*.js' 'lib/**/*.js' 'scripts/**/*.js' 'tasks/**/*.js' '*.js'",
    "live": "node server.js",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "prebuild": "rm -rf build && mkdir -p build/lib",
    "precommit": "zapier-scripts format-staged",
    "prepublish": "npm run build",
    "start": "node server.js",
    "test": "jest --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:zapier/formatic.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^3.3.7",
    "codemirror": "^5.32.0",
    "css-loader": "^0.28.7",
    "envify": "^4.1.0",
    "es6-promise": "^4.1.1",
    "eslint-loader": "^2.0.0",
    "groc": "https://registry.npmjs.org/groc/-/groc-0.7.0.tgz",
    "gulp": "^3.9.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-load-plugins": "^1.5.0",
    "gulp-run": "^1.7.1",
    "gulp-shell": "^0.6.3",
    "jest-cli": "^21.2.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "marked": "^0.3.9",
    "node-libs-browser": "^2.1.0",
    "open": "^0.0.5",
    "portscanner": "^2.1.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-tools": "^0.13.3",
    "require-dir": "^0.3.2",
    "rollup": "^0.52.0",
    "rollup-plugin-babel": "^3.0.2",
    "run-sequence": "^2.2.0",
    "shelljs": "^0.7.8",
    "style-loader": "^0.19.0",
    "uglify-js": "^3.2.0",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.9.5",
    "zapier-scripts": "^3.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "create-react-class": "^15.6.2",
    "deep-equal": "^1.0.1",
    "draft-js": "^0.10.5",
    "immutability-helper": "^2.5.1",
    "object-assign": "^2.1.1",
    "prop-types": "^15.6.0",
    "react-scroll-lock": "git+https://github.com/Laiff/react-scroll-lock.git#267bf5bcf84d334aecc20908657088007b698dc2",
    "react-transition-group": "^1.2.1"
  },
  "peerDependencies": {
    "react": ">= 0.16",
    "react-dom": ">= 16.0.0"
  },
  "jest": {
    "preprocessorIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "automock": false,
    "testPathDirs": [
      "__tests__"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setup-jest-env.js"
  }
}
